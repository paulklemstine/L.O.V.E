{
    "github": {
        "command": "docker",
        "args": [
            "run",
            "-i",
            "--rm",
            "-e",
            "GITHUB_PERSONAL_ACCESS_TOKEN",
            "ghcr.io/github/github-mcp-server"
        ],
        "requires_env": [
            "GITHUB_PERSONAL_ACCESS_TOKEN"
        ],
        "tools": {
            "repos.search_repositories": "Search for repositories on GitHub.",
            "repos.get_file_contents": "Get the contents of a file or directory in a repository.",
            "repos.list_commits": "List commits in a repository.",
            "issues.search_issues": "Search for issues on GitHub.",
            "pull_requests.search_pull_requests": "Search for pull requests on GitHub.",
            "users.search_users": "Search for users on GitHub."
        }
    }
}